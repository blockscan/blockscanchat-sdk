{
  "name": "blockscanchat-sdk",
  "version": "1.0.8",
  "description": "NPM Package for Blockscan Chat APIs",
  "main": "./src/index.js",
  "keywords": [
    "sdk",
    "blockscan",
    "blockscan chat",
    "blockscan-chat"
  ],
  "scripts": {
    "prepare": "husky install",
    "commit": "cz",

    "test": "jest",
    "update:packages": "npm install git+ssh://git@github.com:blockscan/blockscanchat-sdk.git#master"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "repository": {
